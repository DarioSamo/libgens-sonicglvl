<?xml version="1.0" ?>
<Template>
  <RangeIn type="float" default="1000" description="Distance from object before it spawns" />
  <RangeOut type="float" default="1200" description="Distance from object before it despawns" />
  <Parent type="target" description="" />
  <Unknown1 type="uint16" default="0" description="" />
  <Unknown2 type="uint32" default="0" description="" />
  <Unknown3 type="float" default="0" description="" />
  <CollisionWidth type="float" default="0" description="collision of width" />
  <CollisionHeight type="float" default="0" description="the height of the collision" />
  <CollisionLength type="float" default="0" description="the length of the collision" />
  <Target type="target" description="associate camera" />
  <IsHigherPhantomCamera type="bool" default="false" description="priority than phantom camera" />
  <Priority type="uint32" default="0" description="priority (higher takes precedence)" />
  <EaseTimeEnter type="float" default="0" description="transition time at the time of invasion (sec)" />
  <EaseTimeLeave type="float" default="0" description="transition time at the time of withdrawal (sec)" />
  <IsEnableCollision type="bool" default="false" description="[unsupported] it may need to do a collision judgment at the time of transition" />
  <InterpolateType type="enum" default="0" description="interpolation method" />
  <NoneEffectiveBoost type="bool" default="false" description="it wants to boost angle without change" />
  <OffTimer type="float" default="0" description="camera start-up time" />
  <Extra type="model" name="Camera_GLVL.model" />
  <Extra type="preview_box_x" name="CollisionWidth" />
  <Extra type="preview_box_y" name="CollisionHeight" />
  <Extra type="preview_box_z" name="CollisionLength" />
</Template>
