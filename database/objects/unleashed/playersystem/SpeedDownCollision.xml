<?xml version="1.0" ?>
<Template>
  <Collision_Height type="float" default="50" description="" />
  <Collision_Length type="float" default="100" description="" />
  <Collision_Width type="float" default="105" description="" />
  <DefaultStatus type="float" default="0" description="" />
  <GroundOffset type="float" default="0" description="" />
  <Shape_Type type="float" default="0" description="0 = Box, 1 = Sphere (only modify Width), 2 = Cylinder" />
  <Speed type="float" default="25" description="Speed at which the player moves while in the collision" />
  <m_IsSign type="bool" default="true" description="Display the shoe prompt" />
  <m_IsRate type="bool" default="false" description="" />
  <m_SpeedRate type="float" default="1" description="" />
  <Extra type="preview_box_x" name="Collision_Width" condition="Shape_Type == 0"/>
  <Extra type="preview_box_y" name="Collision_Height" condition="Shape_Type == 0"/>
  <Extra type="preview_box_z" name="Collision_Length" condition="Shape_Type == 0"/>
  <Extra type="preview_sphere" name="Collision_Width" condition="Shape_Type == 1"/>
  <Extra type="preview_cylinder_x" name="Collision_Width" condition="Shape_Type == 2"/>
  <Extra type="preview_cylinder_y" name="Collision_Height" condition="Shape_Type == 2"/>
  <Extra type="description" name="A volume which slows the player." />
</Template>
